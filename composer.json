{
    "name": "edrisaturay/filament-utilities",
    "description": "Reusable functions, fields, filters and columns for Filament.",
    "homepage": "https://github.com/edrisaturay/filament-utilities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edrisa A Turay",
            "email": "edrisa@edrisa.com"
        }
    ],
    "support": {
        "issues": "https://github.com/edrisaturay/filament-utilities/issues",
        "source": "https://github.com/edrisaturay/filament-utilities"
    },
    "require": {
        "php": "^8.3",
        "filament/filament": "^5.0",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "EdrisaTuray\\FilamentUtilities\\": "src/"
        }
    },
    "extra": {
        "laravel": {
